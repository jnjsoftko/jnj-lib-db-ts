{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    // "incremental": true,
    "noImplicitAny": false,
    "module": "esnext",
    "target": "esnext",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["esnext", "es2022"]
  }
}
