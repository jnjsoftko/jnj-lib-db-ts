{
  "include": ["src/**/*.ts", "src/pbRest.ts_"],
  "compilerOptions": {
    // "incremental": true,
    "target": "ES2022",
    "module": "ES2022",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ES2021.String",
      "esnext",
      "es2018",
      "es2017",
      "es2016",
      "es2015",
      "es7",
      "es6",
      "es5",
      "dom"
    ]
  }
}
